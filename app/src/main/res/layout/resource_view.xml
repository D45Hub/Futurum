<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#131313"
    android:backgroundTint="#131313">

    <ImageView
        android:id="@+id/ResourceImg"
        android:layout_width="54dp"
        android:layout_height="54dp"
        android:layout_marginStart="80dp"
        android:layout_marginTop="80dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/ic_launcher_foreground" />

    <TextView
        android:id="@+id/FactoryOveriew"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="80dp"
        android:layout_marginTop="20dp"
        android:fontFamily="@font/muli"
        android:text="[Name]: [Level]"
        android:textColor="@color/plainWhite"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Production" />

    <TextView
        android:id="@+id/ResourceName"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginTop="16dp"
        android:text="ResourceName"
        android:textColor="@color/plainWhite"
        app:layout_constraintStart_toEndOf="@+id/ResourceImg"
        app:layout_constraintTop_toTopOf="@+id/ResourceImg" />

    <TextView
        android:id="@+id/WarehouseStock"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="80dp"
        android:layout_marginTop="64dp"
        android:fontFamily="@font/muli"
        android:text="WarehouseStock"
        android:textColor="@color/plainWhite"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/ResourceName" />

    <TextView
        android:id="@+id/Production"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="80dp"
        android:layout_marginTop="20dp"
        android:fontFamily="@font/muli"
        android:text="Productionrate: [prodrate]"
        android:textColor="@color/plainWhite"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/WarehouseStock" />

    <ImageButton
        android:id="@+id/clickButton"
        android:layout_width="92dp"
        android:layout_height="69dp"
        android:background="@drawable/click_button"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.915"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.679"
        android:soundEffectsEnabled="false" />

    <ImageButton
        android:id="@+id/backButton2"
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:background="@drawable/back_button"
        android:soundEffectsEnabled="false"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.9" />

    <ImageButton
        android:id="@+id/factoryButton"
        android:layout_width="92dp"
        android:layout_height="69dp"
        android:background="@drawable/factory_button"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.084"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.679"
        android:soundEffectsEnabled="false" />

    <ImageButton
        android:id="@+id/Collect"
        android:layout_width="104dp"
        android:layout_height="69dp"
        android:background="@drawable/collect_button"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.679"
        android:soundEffectsEnabled="false" />

</androidx.constraintlayout.widget.ConstraintLayout>